{
  "PowerPositionsExportSettings": {
    "ReportPath": "/Users/neven/PowerPositions",
    "IntervalInMinutes": "1"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/production-log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
